{
  "name": "cec-tracker-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build-gql": "ts-node ./src/generate-typings",
    "build": "nest build && npm run copy-gql-types && npm run copy-gql-queries",
    "build:dev": "env-cmd -f .env.dev npm run build",
    "build:prod": "env-cmd -f .env.prod npm run build",
    "build-container": "nest build && npm run copy-gql-types && npm run copy-gql-queries",
    "gcp-build": "npm run build:prod",
    "predeploy:prod": "copy .env.prod .env",
	"predeploy:dev": "copy .env.dev .env",
    "copy-gql-types": "copyfiles -f \"src/shared/graphql/types/*.graphql\" \"dist/shared/graphql/types\" && echo 'GQL types copied'",
    "copy-gql-queries": "copyfiles -f \"src/shared/graphql/queries/*.graphql\" \"dist/shared/graphql/queries\" && echo 'GQL queries copied'",
	"deploy:dev": "npm run build:dev && copy .env.dev .env && gcloud app deploy app.dev.yaml --quiet --project=cec-dev-368607",
    "deploy:prod2": "copy .env.prod .env && gcloud app deploy app.prod.yaml --quiet --project=cec-tracker-prod-370901",
    "deploy:prod": "npm run build:prod && copy .env.prod .env && gcloud app deploy app.prod.yaml --quiet --project=cec-tracker-prod-370901",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "env-cmd -f .env node dist/main",
    "start:local": "env-cmd -f .env.local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:dev": "env-cmd -f .env.dev nest start --watch",
    "start:prod": "node dist/main",
    "start:prod-container": "nest start && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.17",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.0.18",
    "@nestjs/platform-express": "^9.0.0",
    "apollo-server-express": "^3.10.0",
    "body": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "copy": "^0.3.2",
    "env-cmd": "^10.1.0",
    "firebase-admin": "^11.0.0",
    "google-auth-library": "^8.1.1",
    "graphql": "^16.5.0",
    "gts": "^3.1.0",
    "helmet": "^5.1.0",
    "json2csv": "^5.0.7",
    "nest-csv-parser": "^2.0.4",
    "parser": "^0.1.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "stream": "^0.0.2",
    "ts-morph": "^15.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
